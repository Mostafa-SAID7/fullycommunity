<?xml version="1.0" encoding="utf-8"?>
<!--
MonsterASP Folder Publish Profile
Publishes to a local folder that can be uploaded to MonsterASP via FTP or File Manager
Recommended for MonsterASP deployment
-->
<Project>
  <PropertyGroup>
    <WebPublishMethod>FileSystem</WebPublishMethod>
    <PublishProvider>FileSystem</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish />
    <LaunchSiteAfterPublish>false</LaunchSiteAfterPublish>
    <ExcludeApp_Data>false</ExcludeApp_Data>
    <publishUrl>bin\Release\net9.0\win-x86\publish\</publishUrl>
    <DeleteExistingFiles>true</DeleteExistingFiles>
    <!-- MonsterASP: Framework-dependent deployment -->
    <TargetFramework>net9.0</TargetFramework>
    <RuntimeIdentifier>win-x86</RuntimeIdentifier>
    <SelfContained>false</SelfContained>
    <!-- Ensure web.config and logs folder are included -->
    <PublishReadyToRun>false</PublishReadyToRun>
    <PublishSingleFile>false</PublishSingleFile>
  </PropertyGroup>
</Project>

